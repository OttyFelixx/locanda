<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="persistence.mybatis.mappers.DownloaderMapper"> 
  <cache />
  
  <insert id="insertDownloader" parameterType="model.Downloader" useGeneratedKeys="true" keyProperty="id">
  	insert into Downloader(name,surname,email,phone) 
  	values(#{name},#{surname},#{email},#{phone})
  </insert>
  
</mapper>